<h2>Songs Table</h2>

<table>
  <thead>
  <tr>
    <th>Song Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
    <td><%= link_to song.title, song %></td>
    <td><%= song.released %></td>
    <td><%= song.release_year %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
  </tr>
  <% end %>
  </tbody>
</table>

<%= link_to "create a song", new_song_path %>
