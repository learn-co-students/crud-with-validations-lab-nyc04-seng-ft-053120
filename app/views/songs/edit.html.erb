<h1>Edit Song</h1>
<% if @song.errors.any? %>
    <% @song.errors.full_messages.each do |m| %>
      <%= m %><br>
    <% end %>
<% end %>

<ul>
<%= form_for @song do |f| %>
  <li>
  <%= f.label :title %>
  <%= f.text_field :title %>
  </li>
  <li>
  <%= f.label :released %>
  <%= f.check_box :released %>
  </li>
  <li>
  <%= f.label :release_year %>
  <%= f.number_field :release_year %>
  </li>
  <li>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  </li>
  <li>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  </li>
  <br>
  <%= f.submit %>
<% end %>
</ul>
